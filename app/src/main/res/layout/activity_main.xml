<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_string"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="String"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_2String"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2String"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_obj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OBJ"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_promise"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Promise"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_nested_promise"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NestedPromise"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_console"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Console Log"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_set_time_out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="setTimeout"
        android:textAllCaps="false" />

</LinearLayout>